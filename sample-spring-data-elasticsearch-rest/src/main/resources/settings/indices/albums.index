{    "settings" : {        "number_of_shards" : 1,        "refresh_interval" : "1s",        "number_of_replicas" : 1,        "store.type": "fs"    },    "aliases" : {        "read-albums" : {},        "write-albums" : {}    },    "mappings": {      "album": {        "properties": {          "artistId": {            "type": "keyword",            "copy_to": "search_fields"          },          "name": {            "type": "text",            "copy_to": "search_fields"          },          "publication": {            "type":   "date",            "format": "yyyy-MM-dd",            "copy_to": "search_fields"          },          "tracks": {            "type": "nested",            "properties": {                "name": {                    "type": "text",                    "copy_to": "search_fields"                },                "number": {                    "type": "integer"                },                "length": {                    "type": "integer"                }            }          },          "suggests": {            "type": "completion"          },          "search_fields": {            "type": "text"          }        }      }    }}